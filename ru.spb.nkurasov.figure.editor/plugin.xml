<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.services">
      <serviceFactory
            factoryClass="ru.spb.nkurasov.figure.editor.service.internal.FigureServiceFactory">
         <service
               serviceClass="ru.spb.nkurasov.figure.editor.service.FigureService"></service>
      </serviceFactory>
      <sourceProvider
            provider="ru.spb.nkurasov.figure.editor.service.internal.FigureSourceProvider">
         <variable
               name="activeFigure"
               priorityLevel="workbench">
         </variable>
      </sourceProvider>
   </extension>

</plugin>
